{"ast":null,"code":"export const isDCCurve = curve => {\n  if (curve === null) {\n    return false;\n  }\n  const length = curve.length;\n  if (length % 2 !== 0) {\n    return curve[Math.floor(length / 2)] !== 0;\n  }\n  return curve[length / 2 - 1] + curve[length / 2] !== 0;\n};","map":{"version":3,"names":["isDCCurve","curve","length","Math","floor"],"sources":["../../../src/helpers/is-dc-curve.ts"],"sourcesContent":[null],"mappings":"AAEA,OAAO,MAAMA,SAAS,GAAwBC,KAAK,IAAI;EACnD,IAAIA,KAAK,KAAK,IAAI,EAAE;IAChB,OAAO,KAAK;;EAGhB,MAAMC,MAAM,GAAGD,KAAK,CAACC,MAAM;EAE3B,IAAIA,MAAM,GAAG,CAAC,KAAK,CAAC,EAAE;IAClB,OAAOD,KAAK,CAACE,IAAI,CAACC,KAAK,CAACF,MAAM,GAAG,CAAC,CAAC,CAAC,KAAK,CAAC;;EAG9C,OAAOD,KAAK,CAACC,MAAM,GAAG,CAAC,GAAG,CAAC,CAAC,GAAGD,KAAK,CAACC,MAAM,GAAG,CAAC,CAAC,KAAK,CAAC;AAC1D,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}